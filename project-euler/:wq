import numpy as np
with open("paths_smol.txt") as f:
  data = f.read().strip().split('\n')

data_matrix = []
for line in data:
  data_matrix.append(np.array([int(i) for i in line.split()]))


def get_max_neighbours(row):
  print(line)
  res = []
  for a, b in zip(row, row[1:]):
    res.append(max(a, b))
 
  return res
    

print(get_max_neighbours(data_matrix[-1]))
